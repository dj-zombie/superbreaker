<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zombieheadstudios.superbreaker" version="1.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBillingPlugin">
        <param name="android-package" value="com.smartmobilesoftware.inappbilling.InAppBillingPlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>Super Breaker</name>
    <description>
        A breakout clone
    </description>
    <author email="kristoole@gmail.com" href="http://zombieheadstudios.com">
        Zombie Head Studios
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
</widget>
